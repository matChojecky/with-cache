{
  "name": "with-cache",
  "version": "1.2.0",
  "description": "Function composition wrapperd that will allow you to cache returned values",
  "type": "commonjs",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && tsc -p tsconfig-d.json",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register tests/**/*.ts",
    "clean": "rm -rf lib",
    "preversion": "npm test",
    "bundlesize": "bundlesize"
  },
  "author": "Mateusz Chojecki <mchojecky@gmail.com> (https://twitter.com/matChojecky)",
  "license": "MIT",
  "devDependencies": {
    "@types/assert": "^1.5.2",
    "@types/mocha": "^8.0.3",
    "assert": "^2.0.0",
    "bundlesize": "^0.18.0",
    "mocha": "^8.1.3",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.6"
  },
  "homepage": "https://github.com/matChojecky/with-cache",
  "repository": {
    "type": "git",
    "url": "https://github.com/matChojecky/with-cache.git"
  },
  "bugs": {
    "url": "https://github.com/matChojecky/with-cache/issues"
  },
  "bundlesize": [
    {
      "path": "./lib/esm/index.js",
      "threshold": "1kB"
    }
  ]
}
